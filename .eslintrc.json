{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "off",
    "max-len": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "no-plusplus": "off",
    "prefer-const": "off",
    "consistent-return": "off",
    "camelcase": "error",
    "indent": "off",
    "no-underscore-dangle": "off",
    "radix": "off",
    "no-restricted-syntax": "off",
    "no-unneeded-ternary": "off",
    "no-await-in-loop": "off",
    "eqeqeq": "off"
  }
}
